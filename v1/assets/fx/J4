=ARRAYFORMULA(
 IF(B4:B="";
  "";
  MAP(ROW(B4:B);
   LAMBDA(r;
    J$3 +
    IFERROR(SUMIFS(INDIRECT("G4:G"&r); INDIRECT("D4:D"&r); "CASH"; INDIRECT("F4:F"&r); '.Settings'!B5); 0) -
    IFERROR(SUMIFS(INDIRECT("H4:H"&r); INDIRECT("D4:D"&r); "CASH"; INDIRECT("F4:F"&r); '.Settings'!B5); 0)
   )
  )
 )
)